<!-- Begin admin/permission_sets/delete.tpl -->
{opentable}
{titlebar colspan=2 title="Remove Permission Sets"}
<tr><td class="label" colspan="2">Are you sure you want to remove this permission set?</td></tr>
<tr>
<td align="center" class="data">
<form method="post" action="?module=admin&action=permission_sets&subaction=delete&setid={$smarty.get.setid}">
<input type="hidden" name="confirm" value="true" />
<input type="submit" value="Confirm" />
</form>
</td>
<td align="center" class="data">
<form method="post" action="?module=admin&action=permission_sets">
<input type="submit" value="Cancel" />
</form>
</td>
</tr>
{closetable}
<!-- End admin/permission_sets/delete.tpl -->

