<!-- Begin users/whois.tpl -->
<br /><br />
{opentable}
{titlebar colspan=2 title=$user.username}
<tr>
<td class="label" width="20%" align="right" valign="top">First Name:</td>
<td class="data" width="80%">{$user.first_name}</td>
</tr>
<tr>
<td class="label" width="20%" align="right" valign="top">Last Name:</td>
<td class="data" width="80%">{$user.last_name}</td>
</tr
<tr>
<td class="label" width="20%" align="right" valign="top">Email:</td>
<td class="data" width="80%">{$user.email}</td>
</tr>
<tr class="label"><td colspan="2"><a href="" onClick="window.close();">Close Window</a></td></tr>
{closetable}
<!-- End users/whois.tpl -->

